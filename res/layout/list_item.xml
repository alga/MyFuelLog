<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout
	android:id="@+id/linearLayout1"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="horizontal">
	<TextView android:padding="10dp" android:textSize="16sp"
	android:id="@+id/item_date" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_weight="2"/>
	<TextView android:id="@+id/item_volume" android:text="TextView"
		android:layout_height="wrap_content" android:layout_width="wrap_content" android:layout_weight="1" android:textSize="12sp"></TextView>
	<TextView android:text="TextView" android:layout_height="wrap_content"
		android:layout_width="wrap_content" android:id="@+id/item_sum" android:layout_weight="1" android:textSize="12sp"></TextView>
</LinearLayout>
